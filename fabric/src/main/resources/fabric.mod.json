{
  "schemaVersion": 1,
  "id": "hexecuteif",
  "version": "${version}",
  "name": "HexecuteIf",
  "description": "HexCasting addon that adds several useful spells.",
  "authors": [
    "n08i40k"
  ],
  "contact": {
    "homepage": "https://n08i40k.github.io/hexecuteif",
    "sources": "https://github.com/n08i40k/hexecuteif"
  },
  "license": "MIT",
  "icon": "assets/hexecuteif/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "ru.n08i40k.hexecuteif.fabric.FabricHexecuteIf"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "ru.n08i40k.hexecuteif.fabric.FabricHexecuteIfClient"
    }],
    "server": [{
      "adapter": "kotlin",
      "value": "ru.n08i40k.hexecuteif.fabric.FabricHexecuteIfServer"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value": "ru.n08i40k.hexecuteif.fabric.datagen.FabricHexecuteIfDatagen"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "ru.n08i40k.hexecuteif.fabric.FabricHexecuteIfModMenu"
    }]
  },
  "mixins": [
    "hexecuteif-common.mixins.json",
    "hexecuteif-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting_dependency}",
    "cloth-config": ">=${versions.clothConfig}"
  },
  "suggests": {
    "modmenu": ">=${versions.modMenu}"
  }
}
